#include <stdio.h>
#include <stdlib.h>
#include <libSockets.h>
#include <commons/config.h>

#define IP "127.0.0.1"
#define PUERTO_EMISOR "6666"
#define BACKLOG 5
#define PACKAGESIZE 32

int main(int argc, char **argv) {
	int serverCliente;
	serverCliente = conectarCliente(IP, PUERTO_EMISOR, BACKLOG);
	char package[PACKAGESIZE];
	int status = 1;

	while (status != 0) {
		status = recv(serverCliente, (void*) package, PACKAGESIZE, 0);
		if (status != 0) {

			printf("Mensaje Recibido\n %s", package);

		}
		/*send(serverMemoria, package, sizeof(package), 0); */

	}

	return 0;
}
