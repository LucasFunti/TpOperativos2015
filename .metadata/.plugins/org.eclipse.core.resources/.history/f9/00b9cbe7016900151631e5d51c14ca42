#include <commons/txt.h>
#include <sys/socket.h>
#include <libSockets.h>

int interpretar(char *linea, serverMemoria){

	switch(linea){
		scanf("%s", linea);
		send(serverMemoria, linea, sizeof(linea), 0);
		break;
	case "leer":
		scanf("%s", linea);
		break;
	case "finalizar":
		break;
	}
}
